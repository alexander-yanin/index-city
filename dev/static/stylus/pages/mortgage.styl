.mortgage-tabs
  width 260px
  height 266px
  border-radius 2px
  background-color #ffffff
  box-shadow 0 1px 4px 0 rgba(0, 0, 0, 0.31)
  margin-bottom 40px

  &-title
    height 50px
    background-color #33beca
    font-family SourceSansPro
    font-size rem(28px)
    padding 7px 20px
    color #fff

  .mortgage-tabs-container
    padding 16px 20px

.mortgage-tab
  display block
  margin 0 auto 8px
  width 220px
  height 40px
  border-radius 2px
  border solid 1px #33beca
  font-family SourceSansPro
  font-size rem(18px)
  color #33beca
  text-align center
  line-height 35px
  cursor pointer
  text-decoration none

  &:last-child
    margin-bottom 0

  &:hover, &:focus
    background-color #33beca
    color #fff
    text-decoration none

  &.active
    background-color #33beca
    color #fff
    text-decoration none

.mortgage-calculate
  width 560px
  height 420px
  margin 0 auto
  border-radius 2px
  background-color #ffffff
  box-shadow 0 1px 4px 0 rgba(0, 0, 0, 0.31)
  border solid 1px rgba(0, 0, 0, 0.05)
  padding 17px 32px 17px 20px

.mortgage-calculate-result
  width 260px
  height 420px
  border-radius 2px
  background-color #ffffff
  box-shadow 0 1px 4px 0 rgba(0, 0, 0, 0.31)
  border solid 1px rgba(0, 0, 0, 0.05)
  padding 15px 22px

.mortgage-calculate-title
  display block
  font-family SourceSansPro
  font-size rem(28px)
  text-align left
  color rgba(0, 0, 0, 0.87)
  margin-bottom 32px

.mortgage-input-group
  display flex
  position relative
  flex-direction row
  justify-content space-between
  margin-bottom 45px

  label
    display flex
    align-items center
    font-family FiraSansRegular
    font-size 16px
    font-weight normal
    font-style normal
    font-stretch normal
    line-height 1.44
    letter-spacing normal
    text-align left
    color rgba(0, 0, 0, 0.87)

  input, select
    width 380px
    background-color transparent

  input[type="text"], select
    height 40px
    border-radius 3px
    background-color #ffffff
    border solid 1px rgba(120, 120, 120, 0.5)
    padding 9px 20px
    font-size rem(16px)
    font-family FiraSansLight
    font-weight 300
    font-style normal
    font-stretch normal
    line-height normal
    letter-spacing normal


.input-range > input[type="range"]
  position absolute
  bottom -3px


input[type="range"]
  //overflow: hidden;
  -webkit-appearance: none;
  background-color: #b6b6b6

input[type='range']::-webkit-slider-runnable-track
  height: 4px;
  -webkit-appearance: none;

input[type='range']::-webkit-slider-thumb
  -webkit-appearance: none;
  width: 28px;
  height: 28px;
  border-radius 50%
  background-color: #ffffff;
  //box-shadow: 0 3px 3px 0 rgba(0, 0, 0, 0.05), 0 2px 2px 0 rgba(0, 0, 0, 0.1), 0 3px 1px 0 rgba(0, 0, 0, 0.05);
  border: solid 0.5px rgba(0, 0, 0, 0.1);
  margin-top -12px
  cursor: ew-resize;
  //box-shadow: -105px 0 0 1px #43e5f7;


.mortgage-result-group
  display flex
  flex-direction: column
  margin-bottom 16px

  .title
    font-family: FiraSansLight;
    font-size: rem(14px)
    font-weight: 300;
    font-style: normal;
    font-stretch: normal;
    line-height: normal;
    letter-spacing: normal;
    text-align: left;
    color: #323232;
    margin-bottom 6px

  .value
    font-family: FiraSansRegular;
    font-size: rem(20px);
    font-weight: normal;
    font-style: normal;
    font-stretch: normal;
    line-height: 0.95;
    letter-spacing: normal;
    text-align: left;
    color: #007580;


.mortgage-form
  &-title
    font-family: SourceSansPro;
    font-size: rem(16px);
    font-weight: normal;
    font-style: normal;
    font-stretch: normal;
    line-height: 1.25;
    letter-spacing: normal;
    text-align: left;
    color: #2f223a;
    margin-bottom 10px

  &-group
    display flex
    flex-direction column
    margin-bottom 9px

    label
      font-family: FiraSansLight;
      font-size: rem(14px);
      font-weight: 300;
      font-style: normal;
      font-stretch: normal;
      line-height: normal;
      letter-spacing: normal;
      text-align: left;
      color: rgba(120, 120, 120, 0.87);

    input[type="text"]
      width 100%
      height: 40px;
      border-radius: 3px;
      background-color: #ffffff;
      border: solid 1px rgba(120, 120, 120, 0.5);

    input[type="submit"]
      height: 40px;
      border-radius: 5px;
      background-color: #007580;
      border: solid 1px rgba(120, 120, 120, 0.5);
      font-family: SourceSansPro;
      font-size: rem(16px);
      font-weight: normal;
      font-style: normal;
      font-stretch: normal;
      letter-spacing: normal;
      color: #ffffff;


.mortgage-information
  background-color #fff
  padding 17px 20px

.mortgage-information-header
  display flex
  flex-direction row
  justify-content space-between
  margin-bottom 20px

  h3
    font-family: SourceSansPro;
    font-size: 28px;
    font-weight: normal;
    font-style: normal;
    font-stretch: normal;
    line-height: normal;
    letter-spacing: normal;
    text-align: left;
    color: rgba(0, 0, 0, 0.87);

  .mortgage-tab
    margin 0

.mortgage-list
  max-width 860px
  margin 0 auto 20px

.mortgage-item
  display flex
  flex-direction row
  width 100%
  margin-bottom 20px
  min-height 190px
  background-color #fff
  box-shadow: 0 1px 4px 0 rgba(0, 0, 0, 0.31);

  .mortgage-item-wrap-img
    width 260px
    flex-shrink: 0

  .mortgage-item__img
    display inline-block
    width 100%

  .mortgage-item-content
    display inline-block
    background-color #fff
    padding 9px 20px 12px

    .mortgage-item__title
    //margin 9px 0 15px 20px
      display flex
      flex-direction row
      justify-content space-between
      margin-bottom 15px
      .name
        font-family: SourceSansPro;
        font-size: rem(20px);
        font-weight: 600;
        font-style: normal;
        font-stretch: normal;
        line-height: normal;
        letter-spacing: normal;
        text-align: left;
        color: #0c0c0c;
      .rate
        font-family: SourceSansPro;
        font-size: rem(20px);
        font-weight: normal;
        font-style: normal;
        font-stretch: normal;
        line-height: normal;
        letter-spacing: normal;
        color: #0c0c0c;
        word-wrap break-word

    .mortgage-item__text
      margin-bottom 6px
      min-height 84px
      font-family FiraSans
      font-size 16px
      font-weight normal
      font-style normal
      font-stretch normal
      line-height 1.75
      letter-spacing normal
      text-align left
      color rgba(50, 50, 50, 0.87)

      a
        font-family: FiraSans;
        font-size: 16px;
        font-weight: normal;
        font-style: normal;
        font-stretch: normal;
        line-height: 1.75;
        letter-spacing: normal;
        text-align: left;
        color: #007580;
        text-decoration underline

    .mortgage-item__link
      display block
      width 187px
      height 38px
      border-radius 2px
      text-align center
      line-height 37px
      background-color #33beca
      color #fff
      text-decoration none
      font-family SourceSansPro
      font-size 18px

.mortgage-help-information
  border-radius: 2px;
  min-height 265px
  background-color: #ffffff;
  box-shadow: 0 1px 4px 0 rgba(0, 0, 0, 0.31);
  border: solid 1px rgba(0, 0, 0, 0.05);
  margin-bottom 40px
  .mortgage-help-information-title
    height: 50px;
    border-radius: 2px;
    background-color: #33beca;
    font-family: SourceSansPro;
    font-size: rem(28px);
    color: #ffffff;
    padding 0 20px
    line-height 48px
  .mortgage-help-information-text
    font-family: FiraSansRegular;
    font-size: rem(16px);
    font-weight: normal;
    font-style: normal;
    font-stretch: normal;
    line-height: 1.88;
    letter-spacing: normal;
    text-align: left;
    color: rgba(50, 50, 50, 0.87);
    padding 16px 20px


.help-information
  .mortgage-item
    margin-bottom 16px

.mortgage-help-information-item__link
  display block
  width: 187px;
  height: 40px;
  color #33beca
  background-color #fff
  border-radius: 2px;
  border: solid 1px #33beca;
  font-family: SourceSansPro;
  font-size: 18px;
  text-align: center;
  line-height 37px
  text-decoration none

  &:hover
    background #33beca
    color #fff
    text-decoration none

.help-information-wrapper
  .mortgage-help-information
    margin-bottom 31px


article.mainfeature
  width: 400px;
  height: 300px;
  border: 20px solid white;
  margin: 30px auto;
  background-color: #d4d4d4;
  -moz-border-radius: 2px;
  -webkit-border-radius: 2px;
  border-radius: 2px;
  box-shadow: 0 1px 2px rgba(0, 0, 0, 0.08);


.sliderset
  margin-top: 70px;


.slider-range
  position: relative;
  margin: 0;
  margin-top: -4px
  width: 100%;
  display: block;
  background: #d3d3d3;
  height: 4px;
  //box-shadow: 0 1px 0px rgba(255, 255, 255, 0.25), inset 0 0 4px rgba(0, 0, 0, 0.9);
  -moz-border-radius: 2px;
  -webkit-border-radius: 2px;
  border-radius: 2px;

.slider-range div.ui-slider-range
  display: block;
  height: 4px;
  -moz-border-radius: 2px;
  -webkit-border-radius: 2px;
  border-radius: 2px;
  -moz-transition-property: background-color;
  -o-transition-property: background-color;
  -webkit-transition-property: background-color;
  transition-property: background-color;
  -moz-transition-duration: 100ms;
  -o-transition-duration: 100ms;
  -webkit-transition-duration: 100ms;
  transition-duration: 100ms;

.slider-range.red div.ui-slider-range
  background-color: #33beca;
  //box-shadow: inset 0 0 4px rgba(163, 18, 0, 0.7), inset 0 0 2px rgba(0, 0, 0, 0.5);

.slider-range.red div.ui-slider-range.hilite, .slider-range.red div.ui-slider-range.dragging
  background-color: #33beca;

.slider-range a.ui-slider-handle
  position: absolute;
  top: -5px;
  margin-left: -9px;
  z-index: 2;
  height: 16px;
  width: 16px;
  -moz-border-radius: 20px;
  -webkit-border-radius: 20px;
  border-radius: 20px;
  background-color: #d4d4d4;
  background-image: url(https://s3.amazonaws.com/codepen/chrome_radial.jpg);
  -moz-background-size: 100% 100%;
  -o-background-size: 100% 100%;
  -webkit-background-size: 100% 100%;
  background-size: 100% 100%;
  border: 0px solid rgba(0, 0, 0, 0.1);
  box-shadow: 0 0px 2px rgba(0, 0, 0, 0.4), inset 0 0px 1px rgba(0, 0, 0, 0.3), 0 1px 2px rgba(0, 0, 0, 0.6), 0 4px 2px rgba(0, 0, 0, 0.2), 0 9px 4px rgba(0, 0, 0, 0.1), inset 0 2px 1px white;
  outline: none;

.slider-range a.ui-slider-handle:after
  content: "";
  position: absolute;
  width: 15px;
  height: 15px;
  left: -1px;
  top: -4px;
  background-color: none;
  -moz-border-radius: 12px;
  -webkit-border-radius: 12px;
  border-radius: 12px;
  background-image: url('data:image/svg+xml;base64,');
  background-size: 100%;
  background-image: -moz-radial-gradient(#ffffff, rgba(255, 255, 255, 0.05), rgba(255, 255, 255, 0));
  background-image: -webkit-radial-gradient(#ffffff, rgba(255, 255, 255, 0.05), rgba(255, 255, 255, 0));
  background-image: radial-gradient(#ffffff, rgba(255, 255, 255, 0.05), rgba(255, 255, 255, 0));


.ui-slider-handle.ui-state-default.ui-corner-all
  width 28px
  height 28px
  top -11px
  background-color #fff
  box-shadow: 0 3px 3px 0 rgba(0, 0, 0, 0.05), 0 2px 2px 0 rgba(0, 0, 0, 0.1), 0 3px 1px 0 rgba(0, 0, 0, 0.05);
  border: solid 0.5px rgba(0, 0, 0, 0.1);


.ui-slider-range.ui-corner-all.ui-widget-header.ui-slider-range-min
  background #33beca

.ui-slider-handle.ui-corner-all.ui-state-default
  border-radius 20px
  outline none
  background #fff