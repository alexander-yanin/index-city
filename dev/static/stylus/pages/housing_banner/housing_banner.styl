.housing-banner
  position relative
  size(100%, 560px)

  .housing-banner-img
    z-index -1
    size(100%)
    absolute(0, 0, 0, 0)


.housing-banner-content
  padding 0 15px

  &__title
    display block
    margin-top 49px
    line-height 1.34
    text-align left
    font-family SourceSansBold
    color #fff
    font-size rem(72px)
    letter-spacing 0.9px
    text-shadow 1px 2px 3px rgba(0, 0, 0, 0.75)

  .service-list
    margin 99px auto 0
    size(100%, inherit)
  .icon
    size(59px, 59px)
    fill transparent
    stroke #fff
  .service-title
    font-size rem(24px)
  .service-description
    font-size rem(14px)

.bread-crumbs
  margin-top 13px
  font-size rem(13px)
  text-align left
  color #ffffff
  span
    padding 0 3px
  a
    font-family PTSans
    font-size rem(13px)
    text-align left
    color #ffffff
    text-decoration underline

.bread-crumbs-content
  margin 25px 0 14px
  padding-left 15px
  color #323232
  a
    color #323232
    &:last-child
      color #33beca